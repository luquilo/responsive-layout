{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "standard",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "linebreak-style": ["error", "windows"] // digunakan untuk CRLF
    // "linebreak-style": ["error", "unix"], digunakan untuk LF
  }
}
